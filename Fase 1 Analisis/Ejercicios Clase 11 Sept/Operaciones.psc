Proceso Operaciones
	
	Definir n1,n2,suma,resta,multi,divi Como Real;
	
	Escribir "Digite un número";
	Leer n1;
	Escribir "Digite otro número";
	Leer n2;
	suma=n1+n2;
	resta=n1-n2;
	multi=n1*n2;
	divi=n1/n2;
	Escribir "La suma del número ",n1," y el número ",n2, " es: ",suma;
	Escribir "La resta es: ", resta;
	Escribir "La multiplicación es : ",multi;
	Escribir  "La división es: ", divi;
	
FinProceso
